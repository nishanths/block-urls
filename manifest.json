{
    "manifest_version": 2,
    "name": "Block URLs",
    "version": "1.0",
    "description": "This extension blocks you from viewing specified URLs",
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "background": {
        "scripts": ["nav.js"]
    },
    "homepage_url": "https://github.com/nishanths/block-urls",
    "permissions": [
        "webNavigation",
        "tabs",
        "storage",
        "<all_urls>"
    ]
}
